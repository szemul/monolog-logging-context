{
    "name": "szemul/monolog-logging-context",
    "description": "Context support for monolog",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "monolog/monolog": "^3.0.0",
        "szemul/logging-error-handling-context": "^1.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.8.5",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Szemul\\MonologLoggingContext\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szemul\\MonologLoggingContext\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
